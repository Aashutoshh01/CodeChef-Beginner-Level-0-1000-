#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int N;
	        cin>>N;
	        if(N>=2 && N<=100)
	        {
	            int A[N],max;
	            for(int i=0;i<N;i++)
	            {
	                cin>>A[i];
	            }
	            max=A[0];
	            for(int i=1;i<N;i++)
	            {
	                if(A[i]>max)
	                {
	                    max=A[i];
	                }
	            }
	            cout<<max<<endl;
	        }
	    }
	}
	return 0;
}
