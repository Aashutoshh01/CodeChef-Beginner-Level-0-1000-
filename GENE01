#include <iostream>
using namespace std;

int main() {
	// your code goes here
	char a,b;
	cin>>a>>b;
	if(a=='R' && b=='R'){
	    cout<<"R"<<endl;
	}
	else if(a=='B' && b=='B'){
	    cout<<"B"<<endl;
	}
	else if(a=='G' && b=='G'){
	    cout<<"G"<<endl;
	}
	else if(a=='R' && b=='B' || a=='B' && b=='R'){
	    cout<<"R"<<endl;
	}
	else if(a=='B' && b=='G' || a=='G' && b=='B'){
	    cout<<"B"<<endl;
	}
	else if(a=='R' && b=='G' || a=='G' && b=='R'){
	    cout<<"R"<<endl;
	}
	return 0;
}
