#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int s=0,r=0,a[7];
	    for(int i=0;i<7;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<7;i++){
	        if(a[i]==1){
	            s++;
	        }
	        else if(a[i]==0){
	            r++;
	        }
	    }
	    if(s>r){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
