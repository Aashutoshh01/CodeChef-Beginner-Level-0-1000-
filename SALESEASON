#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X;
	        if(X>=1 && X<=10000)
	        {
	            if(X<=100)
	            {
	                cout<<X<<endl;
	            }
	            else if(X>100 && X<=1000)
	            {
	                cout<<X-25<<endl;
	            }
	            else if(X>1000 && X<=5000)
	            {
	                cout<<X-100<<endl;
	            }
	            else 
	            {
	                cout<<X-500<<endl;
	            }
	        }
	    }
	}
	return 0;
}
