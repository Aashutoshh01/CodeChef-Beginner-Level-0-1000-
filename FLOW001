#include <bits/stdc++.h> 

using namespace std;

int main() {
	// Read the number of test cases.
	int T,A,B,i;
	cin>>T;
	if(T>=1 & T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B;
	        if(A>=0 & A<=10000 & B>=0 & B<=10000)
	        {
	            cout<<A+B<<endl;
	        }
	        cout<<endl;
	    }
	}
	return 0;

}
