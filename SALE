#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,min;
	    cin>>a>>b>>c;
	    if(a<=b && a<=c){
	        min=a;
	    }
	    else{
	        if(b<=c){
	            min=b;
	        }
	        else{
	            min=c;
	        }
	    }
	    cout<<a+b+c-min<<endl;
	}
	return 0;
}
