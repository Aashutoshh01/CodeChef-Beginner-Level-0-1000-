#include <iostream>
using namespace std;

int main() {
    int T,X,Y,i;
    cin>>T;
    if(T>=1 && T<=1000)
    {
        for(int i=0;i<T;i++)
        {
            cin>>X>>Y;
            if(X>=0 && X<=100 && Y>=0 && Y<=100)
            {
                if(X>=Y)
                {
                    cout<<X<<endl;
                }
                else
                {
                    cout<<Y<<endl;
                }
            }
        }
    }
	return 0;
}
