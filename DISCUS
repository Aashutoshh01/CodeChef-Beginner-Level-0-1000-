#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C;
	        if(A>=1 && A<=100 && B>=1 && B<=100 && C>=1 && C<=100)
	        {
	            if(A>B && A>C)
	            {
	                cout<<A<<endl;
	            }
	            else
	            {
	                if(B>C)
	                {
	                    cout<<B<<endl;
	                }
	                else
	                {
	                    cout<<C<<endl;
	                }
	            }
	        }
	    }
	}
	return 0;
}
