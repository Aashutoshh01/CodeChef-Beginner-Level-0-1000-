#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b;
	    cin>>a>>b;
	    if(a==b){
	        cout<<"YES"<<endl;
	    }
	    else if(a>b){
	        while(a>b){
	            b*=2;
	        }
	        if(a==b){
	            cout<<"YES"<<endl;
	        }
	        else{
	            cout<<"NO"<<endl;
	        }
	    }
	    else if(b>a){
	        while(b>a){
	            a*=2;
	        }
	        if(a==b){
	            cout<<"YES"<<endl;
	        }
	        else{
	            cout<<"NO"<<endl;
	        }
	    }
	}
	return 0;
}
