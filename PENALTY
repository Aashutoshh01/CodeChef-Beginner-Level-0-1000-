#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a[10],p=0,q=0;
	    for(int i=0;i<10;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<10;i++){
	        if(i%2==0){
	            p+=a[i];
	        }
	        else{
	            q+=a[i];
	        }
	    }
	    if(p>q){
	        cout<<"1"<<endl;
	    }
	    else if(q>p){
	        cout<<"2"<<endl;
	    }
	    else{
	        cout<<"0"<<endl;
	    }
	}
	return 0;
}
