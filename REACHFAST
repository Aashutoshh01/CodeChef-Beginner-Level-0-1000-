#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,y,k;
	    cin>>x>>y>>k;
	    if(x>=y){
	        if((x-y)%k==0){
	            cout<<(x-y)/k<<endl;
	        }
	        else{
	            cout<<((x-y)/k)+1<<endl;
	        }
	    }
	    else{
	        if((y-x)%k==0){
	            cout<<(y-x)/k<<endl;
	        }
	        else{
	            cout<<((y-x)/k)+1<<endl;
	        }
	    }
	}
	return 0;
}
