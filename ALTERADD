#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long int a,b;
	    long int temp;
	    cin>>a>>b;
	    temp=b-a;
	    if(temp%3==0 || (temp-1)%3==0){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
