#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=1 && X<=10000 && Y>=1 && Y<=10000)
	        {
	            if(X<Y)
	            {
	                cout<<"REPAIR"<<endl;
	            }
	            else if(Y<X)
	            {
	                cout<<"NEW PHONE"<<endl;
	            }
	            else
	            {
	                cout<<"ANY"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
