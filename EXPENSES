#include <iostream>
#include<math.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	    int N,X;
	    cin>>N>>X;
	    if(N>=1 && N<20 && X>1 && X<=20)
	    {
	        cout<<pow(2,X-N)<<endl;
	    }
	    }
	}
	return 0;
}
