#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C;
	        if(A>=1 && B>=1 && C>=1 && A<=1000 && B<=1000 && C<=1000)
	        {
	            if(A!=B && B!=C && C!=A)
	            {
	                if(A>B && A>C)
	                {
	                    cout<<"ALICE"<<endl;
	                }
	                else
	                {
	                    if(B>C)
	                    {
	                        cout<<"BOB"<<endl;
	                    }
	                    else
	                    {
	                        cout<<"CHARLIE"<<endl;
	                    }
	                }
	            }
	        }
	    }
	}
	return 0;
}
