#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C;
	        if(A>=-10 && A<=10 && B>=-10 && B<=10 && C>=-10 && C<=10)
	        {
	            if(A*B<0)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if(B*C<0)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if(C*A<0)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
