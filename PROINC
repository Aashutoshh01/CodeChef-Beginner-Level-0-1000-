#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=1 && X<=1000 && Y>=1 && Y<=100 && X%100==0)
	        {
	            cout<<110*X/100-(X-Y)<<endl;;
	        }
	    }
	}
	return 0;
}
