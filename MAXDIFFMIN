#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,i;
	cin>>T;
	if(T>=1 && T<=10)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C;
	        if(A>=1 && A<=8 && B>=2 && B<=9 && C>=3 && C<=10)
	        {
	            cout<<C-A<<endl;
	        }
	    }
	}
	return 0;
}
