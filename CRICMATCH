#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,M,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>M;
	        if(N>=1 && N<=1000 && M>=1 && M<=100)
	        {
	            if(N<=6*M*6)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
