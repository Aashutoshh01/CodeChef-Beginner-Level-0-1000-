#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long int n;
	    long int count=0;
	    cin>>n;
	    if(n%7==0){
	        cout<<n/7<<endl;
	    }
	    else if((n+1)%7==0){
	        cout<<1+n/7<<endl;
	    }
	    else{
	        cout<<n/7<<endl;
	    }
	}
	return 0;
}
