#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,X,K;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>X>>K;
	        if(N>=1 && N<=100 && X>=1 && X<=100 && K>=1 && K<=10000)
	        {
	            if(K>=N*X)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
