#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,j=0;
	    cin>>n;
	    for(int i=1;i<=n;i++){
	        if(n%i==0){
	            j++;
	        }
	    }
	    if(j==2){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
