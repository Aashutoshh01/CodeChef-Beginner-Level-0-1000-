#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int X;
	        cin>>X;
	        if(X>=1 && X<=1000)
	        {
	            if(X%5!=0)
	            {
	                cout<<"-1"<<endl;
	            }
	            else
	            {
	                cout<<(X/10)+(X-(X/10)*10)/5<<endl;
	            }
	        }
	    }
	}
	return 0;
}
