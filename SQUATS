#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X;
	        if(X>=1 && X<=100000)
	        {
	            cout<<15*X<<endl;
	        }
	    }
	    cout<<endl;
	}
	
	return 0;
}
