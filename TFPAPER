#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,K,i;
	cin>>T;
	if(T>=1 && T<=2000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>K;
	        if(N>=1 && N<=100)
	        {
	            if(K>=0 && K<=N)
	            {
	                cout<<N-K<<endl;
	            }
	        }
	    }
	}
	return 0;
}
