#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int l,b;
	int area,perimeter;
	cin>>l>>b;
	area=l*b;
	perimeter=2*(l+b);
	if(area>perimeter){
	    cout<<"Area"<<endl;
	    cout<<area<<endl;
	}
	else if(perimeter>area){
	    cout<<"Peri"<<endl;
	    cout<<perimeter<<endl;
	}
	else{
	    cout<<"Eq"<<endl;
	    cout<<area<<endl;
	}
	return 0;
}
