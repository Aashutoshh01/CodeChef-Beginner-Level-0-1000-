#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=1 && X<=100000 && Y>=1 && Y<=100000)
	        {
	            if(X>=Y)
	            {
	                cout<<"0"<<endl;
	            }
	            else
	            {
	                cout<<Y-X<<endl;
	            }
	        }
	    }
	}
	return 0;
}
