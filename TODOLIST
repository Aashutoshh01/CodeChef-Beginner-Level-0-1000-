#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,D[1000],i,j=0;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N;
	        if(N>=1 && N<=1000)
	        {
	            for(int i=0;i<N;i++)
	            {
	                cin>>D[i];
	            }
	            for(int i=0;i<N;i++)
	            {
	                if(D[i]>=1 && D[i]<=5000)
	                {
	                    if(D[i]<1000)
	                    {
	                        j++;
	                    }
	                }
	            }
	            cout<<N-j<<endl;
	            j=0;
	        }
	    }
	}
	return 0;
}
