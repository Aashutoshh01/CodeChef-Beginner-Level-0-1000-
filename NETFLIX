#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,X,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C>>X;
	        if(A>=1 && A<=100 && B>=1 && B<=100 && C>=1 && C<=100 && X>=1 && X<=100)
	        {
	            if(A+B>=X)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if(B+C>=X)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if(C+A>=X)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
