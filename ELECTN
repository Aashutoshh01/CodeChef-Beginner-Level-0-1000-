#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,A[100],X,i,j,a=0;
	cin>>T;
	if(T>=1 && T<=200)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>X;
	        cout<<endl;
	        if(N>=1 && N<=100 && X>=1 && X<=100)
	        {
	            for(int j=0;j<N;j++)
	            {
	                cin>>A[j];
	            }
	            for(int j=0;j<N;j++)
	            {
	                if(A[j]>=1 && A[j]<=100)
	                {
	                    if(A[j]>=X)
	                    {
	                        a++;
	                    }
	                }
	            }
	            cout<<a;
	            a=0;
	        }
	    }
	}
	return 0;
}
