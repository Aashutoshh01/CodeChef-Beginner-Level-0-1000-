#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N1,N2,N3,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N1>>N2>>N3;
	        if(N1>=1 && N2>=1 && N3>=1 && N1<=100 && N2<=100 && N3<=100)
	        {
	            if(N1>N2+N3)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if(N2>N1+N3)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if(N3>N1+N2)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
