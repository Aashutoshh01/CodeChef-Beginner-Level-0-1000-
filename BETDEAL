#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B;
	        if(A>=1 && A<=100 && B>=1 && B<=100){
	        if(100-A<200-2*B)
	        {
	            cout<<"FIRST"<<endl;
	        }
	        else if(200-2*B<100-A)
	        {
	            cout<<"SECOND"<<endl;
	        }
	        else
	        {
	            cout<<"BOTH"<<endl;
	        }
	    }
	    }
	}
	return 0;
}
