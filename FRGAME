#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    if(b>a){
	        a=a+c;
	        if(b>a){
	            a=a+d;
	            if(b>a){
	                cout<<"S"<<endl;
	            }
	            else{
	                cout<<"N"<<endl;
	            }
	        }
	        else{
	            b=b+d;
	            if(b>a){
	                cout<<"S"<<endl;
	            }
	            else{
	                cout<<"N"<<endl;
	            }
	        }
	    }
	    else{
	        b=b+c;
	        if(b>a){
	            a=a+d;
	            if(b>a){
	                cout<<"S"<<endl;
	            }
	            else{
	                cout<<"N"<<endl;
	            }
	        }
	        else{
	            b=b+d;
	            if(b>a){
	                cout<<"S"<<endl;
	            }
	            else{
	                cout<<"N"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
