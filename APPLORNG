#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int X,A,B;
	cin>>X;
	if(X>=1 && X<=100000)
	{
	    cin>>A>>B;
	    if(A>=1 && A<=100000 && B>=1 && B<=100000)
	    {
	        if(X>=A+B)
	        {
	            cout<<"YES"<<endl;
	        }
	        else
	        {
	            cout<<"NO"<<endl;
	        }
	    }
	}
	return 0;
}
