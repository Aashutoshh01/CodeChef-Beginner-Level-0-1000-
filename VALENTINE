#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int X,Y;
	        cin>>X>>Y;
	        if(X>=1 && X<=100 && Y>=1 && Y<=100)
	        {
	            cout<<X/Y<<endl;
	        }
	    }
	}
	return 0;
}
