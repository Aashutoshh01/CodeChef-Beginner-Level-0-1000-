#include <bits/stdc++.h> 

using namespace std;

int main() {
	int	N,K,i,A[10000000],j=0;
	cin>>N>>K;
	if(N>=1 && N<=10000000 && K>=1 && K<=10000000)
	{
	    for(int i=0;i<N;i++)
	    {
	        cin>>A[i];
	    }
	    for(int i=0;i<N;i++)
	    {
	        if(A[i]>=1 && A[i]<=1000000000)
	        {
	            if(A[i]%K==0)
	            {
	                j++;
	            }
	        }
	    }
	    cout<<j<<endl;
	}
	return 0;
}
