#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,j;
	    cin>>n;
	    int a[n];
	    j=n-1;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    while(a[j]==0){
	        if(a[j]==0){
	            j--;
	        }
	    }
	    cout<<j<<endl;
	}
	return 0;
}
