#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X;
	        if(X>=1 && X<=1000)
	        {
	            if(X<=300)
	            {
	                cout<<"3000"<<endl;
	            }
	            else
	            {
	                cout<<X*10<<endl;
	            }
	        }
	    }
	}
	return 0;
}
