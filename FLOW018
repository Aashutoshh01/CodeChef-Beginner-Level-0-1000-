#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int N,fact=1;
	        cin>>N;
	        if(N>=0 && N<=20)
	        {
	            if(N==0 || N==1)
	            {
	                cout<<"1"<<endl;
	            }
	            else
	            {
	                for(int i=2;i<=N;i++)
	                {
	                    fact=fact*i;
	                }
	                cout<<fact<<endl;
	            }
	        }
	    }
	}
	return 0;
}
