#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N;
	        if(N>=2 && N<=1000)
	        {
	            if(N%4==0)
	            {
	                cout<<N/4<<endl;
	            }
	            else
	            {
	                cout<<(N/4)+1<<endl;
	            }
	        }
	    }
	}
	return 0;
}
