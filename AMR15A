#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int N,A[100],i,a=0,b=0;
	cin>>N;
	if(N>=1 && N<=100)
	{
	    for(int i=0;i<N;i++)
	    {
	        cin>>A[i];
	    }
	    for(int i=0;i<N;i++)
	    {
	        if(A[i]>=1 && A[i]<=100)
	        {
	            if(A[i]%2==0)
	            {
	                a++;
	            }
	            else
	            {
	                b++;
	            }
	        }
	    }
	    if(a>b)
	    {
	        cout<<"READY FOR BATTLE"<<endl;
	    }
	    else
	    {
	        cout<<"NOT READY"<<endl;
	    }
	}
	return 0;
}
