#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        int N,X,K;
	        cin>>N>>X>>K;
	        if(N>=1 && N<=100000 && X>=1 && X<=100000 && K>=0 && K<=100000)
	        {
	            if(K>=N*X)
	            {
	                cout<<N<<endl;
	            }
	            else
	            {
	                cout<<K/X<<endl;
	            }
	        }
	    }
	}
	return 0;
}
