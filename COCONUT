#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    int count=0;
	    if(c%a==0){
	        count+=c/a;
	    }
	    else{
	        count+=((c/a)+1);
	    }
	    if(d%b==0){
	        count+=d/b;
	    }
	    else{
	        count+=((d/b)+1);
	    }
	    cout<<count<<endl;
	}
	return 0;
}
