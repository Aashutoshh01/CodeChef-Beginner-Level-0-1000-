#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,t,p,q,r;
	    cin>>a>>b>>c>>t>>p>>q>>r;
	    if(p>=a && q>=b && r>=c && (p+q+r)>=t){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
