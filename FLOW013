#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int A,B,C;
	        cin>>A>>B>>C;
	        if(A>=1 && A<=180 && B>=1 && B<=180 && C>=1 && C<=180)
	        {
	            if(A+B+C==180)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
