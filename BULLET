#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,Z,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y>>Z;
	        if(X>=1 && X<=100 && Y>=1 && Y<=100 && Z>=1 && Z<=100 && Y%X==0)
	        {
	            if(Y/X<Z)
	            {
	                cout<<Z-Y/X<<endl;
	            }
	            else
	            {
	                cout<<"0"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
