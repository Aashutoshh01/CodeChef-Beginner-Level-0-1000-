#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,M,K,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>M>>K;
	        if(N>=1 && N<=100 && M>=1 && M<=100)
	        {
	            if(K>=0 && K<=M)
	            {
	                if(N<=M-K)
	                {
	                    cout<<"YES"<<endl;
	                }
	                else
	                {
	                    cout<<"NO"<<endl;
	                }
	            }
	        }
	    }
	}
	return 0;
}
