#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,k,j=0;
	    cin>>n>>k;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	        }
	        for(int i=0;i<n;i++){
	            if((a[i]+k)%7==0){
	            j++;
	        }
	    }
	    cout<<j<<endl;;
	}
	return 0;
}
