#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N;
	        if(N>=1 && N<=100)
	        {
	            if(N%3==0)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
