#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,K,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>K;
	        if(N>=1 && N<=100 && K>=1 && K<=100)
	        {
	            if(K>=N+1)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
