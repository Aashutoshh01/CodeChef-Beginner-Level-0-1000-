#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    int flag=0;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        while(a[i+1]==a[i]){
	          flag++;
	          i++;
	        }
	        }
	        	    cout<<n-flag<<endl;
	    }
	return 0;
}
