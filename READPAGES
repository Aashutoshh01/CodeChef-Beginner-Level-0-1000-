#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,X,Y,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>X>>Y;
	        if(N>=1 && N<=100 && X>=1 && X<=10 && Y>=1 && Y<=10)
	        {
	            if(N<=X*Y)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
