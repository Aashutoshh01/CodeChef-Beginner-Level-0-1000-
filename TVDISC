#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,D,i;
	cin>>T;
	if(T>=1 && T<=5000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C>>D;
	        if(A>=1 && A<=100 && B>=1 && B<=100)
	        {
	            if(C>=0 && C<=A &&  D>=0 && D<=B)
	            {
	                if(A-C<B-D)
	                {
	                    cout<<"FIRST"<<endl;
	                }
	                else if(B-D<A-C)
	                {
	                    cout<<"SECOND"<<endl;
	                }
	                else
	                {
	                    cout<<"ANY"<<endl;
	                }
	            }
	        }
	    }
	}
	return 0;
}
