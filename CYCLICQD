#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=10000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int A,B,C,D;
	        cin>>A>>B>>C>>D;
	        if(A>=1 && A<=357 && B>=1 && B<=357 && C>=1 && C<=357 && D>=1 && D<=357 && A+B+C+D==360)
	        {
	            if(A+C==180 && B+D==180)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
