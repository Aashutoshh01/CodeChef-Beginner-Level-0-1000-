#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int P1,P2,P3,P4,i=0;
	cin>>P1>>P2>>P3>>P4;
	if(P1>=1 && P2>=1 && P3>=1 && P4>=1 && P1<=100 && P2<=100 && P3<=100 && P4<=100)
	{
	    if(P1>=10)
	    {
	        i++;
	    }
	    if(P2>=10)
	    {
	        i++;
	    }
	    if(P3>=10)
	    {
	        i++;
	    }
	    if(P4>=10)
	    {
	        i++;
	    }
	}
	cout<<i<<endl;
	return 0;
}
