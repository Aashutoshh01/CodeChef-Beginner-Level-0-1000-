#include <iostream>
using namespace std;

int main() {
    int T,X,i;
    cin>>T;
    if(T>=1 && T<=2000)
    {
        for(int i=0;i<T;i++)
        {
            cin>>X;
            if(X>=1 && X<=4000)
            {
                if(X>=2000)
                {
                    cout<<"YES"<<endl;
                }
                else
                {
                    cout<<"NO"<<endl;
                }
            }
        }
    }
	return 0;
}
