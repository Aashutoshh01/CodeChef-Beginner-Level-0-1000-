#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=1 && Y>=1 && X<=10 && Y<=10)
	        {
	            if(2*X>5*Y)
	            {
	                cout<<"CHOCOLATE"<<endl;
	            }
	            else if(5*Y>2*X)
	            {
	                cout<<"CANDY"<<endl;
	            }
	            else
	            {
	                cout<<"EITHER"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
