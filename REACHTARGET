#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,Y,X,i;
	cin>>T;
	if(T>=1 && T<=10)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=50 && X<=200 && Y>=50 && Y<=200)
	        {
	            cout<<X-Y<<endl;
	        }
	    }
	}
	return 0;
}
