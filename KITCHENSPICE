#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X;
	        if(X>=1 && X<=10)
	        {
	            if(X<4)
	            {
	                cout<<"MILD"<<endl;
	            }
	            else if(X>=4 && X<7)
	            {
	                cout<<"MEDIUM"<<endl;
	            }
	            else
	            {
	                cout<<"HOT"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
