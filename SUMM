#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C;
	        if(A>=0 && B>=0 && C>=0 && A<=100 && B<=100 && C<=100)
	        {
	            if(A+B==C)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
