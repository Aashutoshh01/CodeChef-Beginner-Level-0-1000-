#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=0 && X<=100 && Y>=0 && Y<=100)
	        {
	            cout<<10*X+5*Y<<endl;
	        }
	    }
	}
	return 0;
}
