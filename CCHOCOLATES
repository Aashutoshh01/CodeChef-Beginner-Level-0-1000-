#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,Z,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y>>Z;
	        if(X>=1 && Y>=1 && Z>=1 && X<=1000 && Y<=1000 && Z<=1000)
	        {
	            cout<<(5*X+10*Y)/Z<<endl;
	        }
	    }
	}
	return 0;
}
