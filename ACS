#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int P;
	        cin>>P;
	        if(P>=0 && P<=1000)
	        {
	            if(P/100+P%100<=10)
	            {
	                cout<<P/100+P%100<<endl;
	            }
	            else
	            {
	                cout<<"-1"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
