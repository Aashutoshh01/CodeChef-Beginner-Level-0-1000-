#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,d,count=0;
	    cin>>a>>b>>c>>d;
	    int r[9]={0};
	    for(int i=a;i<=b;i++){
	        if(r[i]==0){
	            r[i]=1;
	        }
	    }
	    for(int i=c;i<=d;i++){
	        if(r[i]==0){
	            r[i]=1;
	        }    
	    }
	    for(int i=0;i<9;i++){
	        if(r[i]==1){
	            count++;
	        }
	    }
	    cout<<count<<endl;
	}
	return 0;
}





