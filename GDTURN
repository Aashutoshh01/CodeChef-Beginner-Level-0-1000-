#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,Y,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=1 && X<=6 && Y>=1 &&Y<=6)
	        {
	            if(X+Y>6)
	            cout<<"YES";
	            else
	            cout<<"NO";
	        }
	        cout<<endl;
	    }
	}
	return 0;
}
