#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int d;
	    string n;
	    cin>>d>>n;
	    int flag=0;
	    for(int i=0;i<d;i++){
	        if(n[i]=='5' || n[i]=='0'){
	            flag++;
	            break;
	        }
	    }
	    if(flag!=0){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
