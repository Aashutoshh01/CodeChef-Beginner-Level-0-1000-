#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,X,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>X;
	        if(N>=1 && N<=100 && X>=1 && X<=10)
	        {
	            cout<<N/(3*X)<<endl;
	        }
	    }
	}
	return 0;
}
