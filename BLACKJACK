#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        int A,B;
	        cin>>A>>B;
	        if(A>=1 && A<=10 && B>=1 && B<=10)
	        {
	            if(21-A-B<=10)
	            {
	                cout<<21-A-B<<endl;
	            }
	            else
	            {
	                cout<<"-1"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
