#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,N,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>N;
	        if(X>=10 && X<=200 && N>=0 && N<=10 && X%10==0)
	        {
	            cout<<((X/10)*N)<<endl;
	        }
	    }
	}
	return 0;
}
