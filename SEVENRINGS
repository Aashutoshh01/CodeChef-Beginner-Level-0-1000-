#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,N,X,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>N>>X;
	        if(N>=1 && N<=1000 && X>=1 && X<=1000)
	        {
	            if(N*X>=10000 && N*X<=99999)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
