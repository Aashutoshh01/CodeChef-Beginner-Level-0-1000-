#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,C,i;
	cin>>T;
	if(T>=1 && T<=100)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>C;
	        if(A>=1 && A<=10 && B>=1 && B<=10 && C>=1 && C<=10)
	        {
	            if((A+B)%2!=0)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if((B+C)%2!=0)
	            {
	                cout<<"YES"<<endl;
	            }
	            else if((C+A)%2!=0)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
