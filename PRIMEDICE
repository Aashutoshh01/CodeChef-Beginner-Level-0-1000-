#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,i;
	cin>>T;
	if(T>=1 && T<=36)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B;
	        if(A>=1 && A<=6 && B>=1 && B<=6)
	        {
	            if(A+B==2 || A+B==3 || A+B==5 || A+B==7 || A+B==11)
	            {
	                cout<<"ALICE"<<endl;
	            }
	            else
	            {
	                cout<<"BOB"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
