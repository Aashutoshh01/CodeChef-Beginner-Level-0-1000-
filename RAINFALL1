#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,i;
	cin>>T;
	if(T>=1 && T<=20)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X;
	        if(X>=1 && X<=20)
	        {
	            if(X<3)
	            {
	                cout<<"LIGHT"<<endl;
	            }
	            else if(X>=7)
	            {
	                cout<<"HEAVY"<<endl;
	            }
	            else
	            {
	                cout<<"MODERATE"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
