#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        int A,B,C;
	        cin>>A>>B>>C;
	        if(A>=1 && A<=1000000 && B>=1 && B<=1000000 && C>=1 && C<=1000000)
	        {
	            if(A>B && A>C)
	            {
	                if(B>C)
	                {
	                    cout<<B<<endl;
	                }
	                else
	                {
	                    cout<<C<<endl;
	                }
	            }
	            if(B>A && B>C)
	            {
	                if(A>C)
	                {
	                    cout<<A<<endl;
	                }
	                else
	                {
	                    cout<<C<<endl;
	                }
	            }
	            if(C>B && C>A)
	            {
	                if(B>A)
	                {
	                    cout<<B<<endl;
	                }
	                else
	                {
	                    cout<<A<<endl;
	                }
	            }
	        }
	    }
	}
	return 0;
}
