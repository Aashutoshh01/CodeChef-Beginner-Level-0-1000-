#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X;
	        if(X>=1 && X<=1000)
	        {
	            if(X%25==0)
	            {
	                cout<<X/25<<endl;;
	            }
	            else 
	            {
	                cout<<(X/25)+1<<endl;
	            }
	        }
	    }
	}
	return 0;
}
