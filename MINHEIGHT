#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,X,H,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>H;
	        if(X>=1 && X<=100 && H>=1 && H<=100)
	        {
	            if(X<H)
	            {
	                cout<<"NO"<<endl;
	            }
	            else
	            {
	                cout<<"YES"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
