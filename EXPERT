#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,i;
	float X,Y;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>X>>Y;
	        if(X>=1 && X<=1000000 && Y>=1 && Y<=1000000)
	        {
	            if(Y>=X/2)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
