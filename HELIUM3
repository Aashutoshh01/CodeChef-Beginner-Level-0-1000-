#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,A,B,X,Y,i;
	cin>>T;
	if(T>=1 && T<=1000)
	{
	    for(int i=0;i<T;i++)
	    {
	        cin>>A>>B>>X>>Y;
	        if(A>=1 && B>=1 && X>=1 && Y>=1 && A<=1000 && B<=1000 && X<=1000 && Y<=1000)
	        {
	            if(X*Y>=A*B)
	            {
	                cout<<"YES"<<endl;
	            }
	            else
	            {
	                cout<<"NO"<<endl;
	            }
	        }
	    }
	}
	return 0;
}
